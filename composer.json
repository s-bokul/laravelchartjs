{
    "name": "s-bokul/laravelchartjs",
    "description": "Simple chartJS package for Laravel 5.x using Chartjs v2 library",
    "keywords": [
        "chartjs","chart","reports","laravel","laravel5"
    ],
    "type": "composer-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Bokul",
            "email": "s.bokul@outlook.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "illuminate/support": "^5.1|^6.0|^7.0"
    },

    "autoload": {
      "psr-4": {
        "SBokul\\LaravelChartJs\\" : "src/"
      }
    },
    
    "extra": {
        "laravel": {
            "providers": [
                "SBokul\\LaravelChartJs\\Providers\\ChartjsServiceProvider"
            ]
        }
    }
}
